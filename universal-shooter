createSection("ðŸ“¦ ESP SETTINGS")
createToggle("ESP Enabled", espEnabled, function(state)
    espEnabled = state
    if not state then
        for player, _ in pairs(espObjects) do
            removeESP(player)
        end
    else
        initializeESP()
    end
end)

