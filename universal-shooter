-- Create World Visuals Section
createSection("ðŸŒ WORLD VISUALS")

-- Theme Selector
local themeFrame = Instance.new("Frame")
themeFrame.Size = UDim2.new(1, -10, 0, 50)
themeFrame.Position = UDim2.new(0, 5, 0, yOffset)
